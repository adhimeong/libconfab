package org.confab;

import java.util.List;
import java.util.ArrayList;

public class ForumThread extends BulletinBoardEntity {
    public String replyCount;
    public String viewCount;
    public boolean isSticky;
    public List<Post> posts;

    public ForumThread(Forum parent) {
        super();
        this.parent = parent;
        this.url = parent.url;
        replyCount = new String();
        viewCount = new String();
        isSticky = false;
        posts = new ArrayList<Post>();
    }
}
